							<JavaScript>
								function resetUrl() {
									wv.goto(chart_url.value);
								}
								module.exports = {
									resetUrl: resetUrl
								};
							</JavaScript>
							<Activated When="Immediate">
								<Callback Handler="{resetUrl}" />
							</Activated>
							<NativeViewHost>
								<WebView ux:Name="wv" Dock="Fill" Url="{chart_url}">

								</WebView>
							</NativeViewHost>
				        </Panel>
					</DockPanel>
