							<Panel ux:Class="MenuItem" Height="MenuItemHeight">
								<Rectangle Alignment="Bottom" Height="2" Color="#fff2" />
							</Panel>
							<!-- MENU -->
							<MenuItem ux:Class="MenuButton" Height="MenuItemHeight" HitTestMode="LocalBounds">
								<string ux:Property="Text" />
								<Text ux:Name="label" Value="{ReadProperty Text}" FontSize="28" Color="ForegroundColor" Alignment="Top" />
								<WhilePressed>
									<Scale Target="label" Factor=".85" Duration=".1" />
								</WhilePressed>
							</MenuItem>
							<!-- MENU ITEMS -->
							<StackPanel ux:Name="normalMenuItems" ItemSpacing="50">
								<MenuButton Alignment="Left" Text="Home" Clicked="{handle_cal_visibility}">
									<Clicked>
										<Set menu_is_open.Value="false" />
										<Set home_page.Visibility="Visible" />
										<Set web_view_content.Visibility="Collapsed" />
										<Set extra_content.Visibility="Collapsed" />
									</Clicked>
								</MenuButton>
								<MenuButton Alignment="Left" Text="Graphs" Clicked="{handle_cal_visibility}">
									<Clicked>
										<Set menu_is_open.Value="false" />
										<Set home_page.Visibility="Collapsed" />
										<Set web_view_content.Visibility="Visible" />
										<Set extra_content.Visibility="Collapsed" />
									</Clicked>
								</MenuButton>
								<MenuButton Alignment="Left" Text="Extra content" Clicked="{handle_cal_visibility}">
									<Clicked>
										<Set menu_is_open.Value="false" />
										<Set home_page.Visibility="Collapsed" />
										<Set web_view_content.Visibility="Collapsed" />
										<Set extra_content.Visibility="Visible" />
									</Clicked>
								</MenuButton>
							</StackPanel>
						</StackPanel>
					</Panel>
