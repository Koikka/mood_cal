									<Rectangle CornerRadius="20" Alignment="Center">
										<Text Alignment="Center" FontSize="21" Color="BackgroundColor" Margin="10, 10, 10, 10" TextAlignment="Center" TransformOrigin="VerticalBoxCenter">Mood calendar</Text>
									</Rectangle>
									<!-- <Button Text="" Margin="20, 20" Padding="20,20" Clicked="{set_calendar_type_to_use}"> -->
									<Button Text="" Margin="5, 10" Padding="20,20">
										<Text Value="Select this" Color="#fff" Alignment="Center" TextAlignment="Center" />
										<Rectangle CornerRadius="4" Layer="Background" Color="BackgroundColor" />
										<Tapped>
											<Set menu_is_open.Value="false" />
											<Set home_page.Visibility="Visible" />
											<Set web_view_content.Visibility="Collapsed" />
											<Set extra_content.Visibility="Collapsed" />
											<RaiseUserEvent EventName="ButtonEvent">
		                                    	<!-- <UserEventArg Name="nameJotain" StringValue="{observable_vaiks}" /> -->
		                                    	<UserEventArg Name="calendar_type" StringValue="mood_calendar" />
		                                   	</RaiseUserEvent>
										</Tapped>
									</Button>
								</StackPanel>
							</Panel>
							<Panel ux:Name="sleep_container" Padding="0,10,0,20" Color="{sleep_container_color}">
