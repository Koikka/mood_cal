							<Text Alignment="Center" FontSize="21" Color="ForegroundColor">Home</Text>
						</Panel>
						<StackPanel Background="#f9f9f9">
							<Image Layer="Background" File="{bg_image}" StretchMode="UniformToFill" />
							<Text Value="Current calendar type is:" Color="#000" Alignment="Center" TextAlignment="Center" />
							<Text Value="{calendar_type}" Color="#000" Alignment="Center" TextAlignment="Center" />
							<Button Text="" Margin="20, 40, 20, 20" Padding="20,20" Clicked="{set_page_according}">
								<Text Value="Siirry kirjaamaan" Color="#fff" Alignment="Center" TextAlignment="Center" />
								<Rectangle CornerRadius="4" Layer="Background" Color="BackgroundColor" />
								<Clicked>
									<Set menu_is_open.Value="false" />
									<Set home_page.Visibility="Collapsed" />
									<Set web_view_content.Visibility="Collapsed" />
									<Set extra_content.Visibility="Collapsed" />
