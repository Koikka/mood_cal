							<Panel Margin="5, 30">
								<Text FontSize="30" Color="BackgroundColor" TextAlignment="Center" Value="Information saved!" />
							</Panel>
							<Panel Width="200">
						    	<Button Alignment="Center" Text="" Margin="20,20" Clicked="{confirmed_save}" Padding="20,10,20,10" Width="100">
						    		<Text Value="OK" Color="#fff" Alignment="Center" TextAlignment="Center" />
						    		<float4 ux:Key="C600" ux:Value="#fff" />
						    		<float4 ux:Key="C700" ux:Value="#fff" />
						    		<Rectangle CornerRadius="4" Layer="Background" Color="BackgroundColor" />
						    		<Clicked>
										<Set menu_is_open.Value="false" />
										<Set home_page.Visibility="Visible" />
										<Set web_view_content.Visibility="Collapsed" />
										<Set extra_content.Visibility="Collapsed" />
						    		</Clicked>
						    	</Button>
						    </Panel>
						</StackPanel>
					</Panel>
				</Panel>
			</WhileTrue>
